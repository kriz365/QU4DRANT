{
  "name": "@superdevhq/client",
  "version": "0.1.51",
  "private": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**"
  ],
  "devDependencies": {
    "@turbo/gen": "^2.5.0",
    "@types/node": "^22.14.0",
    "eslint": "^9.25.0",
    "tsup": "^8.4.0",
    "typescript": "5.8.2",
    "@superdev/eslint-config": "0.0.1",
    "@superdev/typescript-config": "0.0.1"
  },
  "dependencies": {
    "axios": "^1.9.0"
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "generate:component": "turbo gen react-component",
    "check-types": "tsc --noEmit",
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch"
  }
}